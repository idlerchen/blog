
$("#changeMyFile").click(function(){
	
	$("#myHabbit").css("visibility","visible");
	
	$("#myFile").css("visibility","visible");
	
	$("#saveChange").css("visibility","visible");
	
	$("#cancelChange").css("visibility","visible");
});

$("#cancelChange").click(function(){
	
	$("#myHabbit").css("visibility","hidden");
	
	$("#myFile").css("visibility","hidden");
	
	$("#saveChange").css("visibility","hidden");
	
	$("#cancelChange").css("visibility","hidden");
	
});




